  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#8B8680",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["string", "string.quoted"],
      "settings": {
        "foreground": "#5A9E5F"
      }
    },
    {
      "scope": ["constant.numeric", "constant.language", "constant.character"],
      "settings": {
        "foreground": "#B65FA3"
      }
    },
    {
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#D14D41"
      }
    },
    {
      "scope": ["keyword.control", "keyword.operator"],
      "settings": {
        "foreground": "#C77A3A"
      }
    },
    {
      "scope": ["entity.name.function", "support.function"],
      "settings": {
        "foreground": "#5B8FC9"
      }
    },
    {
      "scope": ["entity.name.type", "entity.name.class", "support.type", "support.class"],
      "settings": {
        "foreground": "#CC785C"
      }
    },
    {
      "scope": ["variable", "variable.other"],
      "settings": {
        "foreground": "#2D2D2D"
      }
    },
    {
      "scope": ["variable.parameter"],
      "settings": {
        "foreground": "#6B6760"
      }
    },
    {
      "scope": ["entity.name.tag"],
      "settings": {
        "foreground": "#D14D41"
      }
    },
    {
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#C77A3A"
      }
    },
    {
      "scope": ["support.constant", "constant.other"],
      "settings": {
        "foreground": "#B65FA3"
      }
    },
    {
      "scope": ["punctuation"],
      "settings": {
        "foreground": "#6B6760"
      }
    },
    {
      "scope": ["meta.embedded", "source.groovy.embedded"],
      "settings": {
        "foreground": "#2D2D2D"
      }
    },
    {
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#D14D41",
        "fontStyle": "underline"
      }
    },
    {
      "scope": ["markup.heading"],
      "settings": {
        "foreground": "#CC785C",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.bold"],
      "settings": {
        "foreground": "#2D2D2D",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.italic"],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["markup.underline.link"],
      "settings": {
        "foreground": "#5B8FC9",
        "fontStyle": "underline"
      }
    }
  ]
}